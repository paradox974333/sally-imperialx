{
  "name": "sally-ai-trading-bot",
  "version": "2.0.0",
  "description": "AI-Powered Cryptocurrency Trading Bot with Dynamic Analysis",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "docker:build": "docker-compose build --no-cache",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f backend",
    "docker:restart": "docker-compose restart backend",
    "health": "curl http://localhost:3000/api/health || echo 'Add health endpoint to server.js'"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "bcryptjs": "^2.4.3",
    "bybit-api": "^3.10.26",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.19.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "keywords": [
    "trading",
    "bot",
    "ai",
    "cryptocurrency",
    "bybit",
    "redis",
    "docker",
    "technical-analysis",
    "openrouter",
    "tavily"
  ],
